        local PlayBtn = Instance.new("TextButton")
        PlayBtn.Parent = Frame
        PlayBtn.Size = UDim2.new(0.4, 0, 0.2, 0)
        PlayBtn.Position = UDim2.new(0.3, 0, 0.75, 0)
        PlayBtn.Text = "Play"
        PlayBtn.TextScaled = true
        PlayBtn.TextColor3 = Color3.new(1, 1, 1)
        PlayBtn.BackgroundColor3 = Color3.new(0, 0, 0)
        PlayBtn.BorderSizePixel = 0
        PlayBtn.Font = Enum.Font.SciFi

        local UICornerButton = Instance.new("UICorner")
        UICornerButton.Parent = PlayBtn
        UICornerButton.CornerRadius = UDim.new(0, 8)

        local currentSound

        local function playSoundLocally(audioID)
            local character = game.Players.LocalPlayer.Character
            if character and character:FindFirstChild("Head") then

                if currentSound then
                    currentSound:Stop()
                    currentSound:Destroy()
                    currentSound = nil
                end

                currentSound = Instance.new("Sound")
                currentSound.Parent = character.Head
                currentSound.SoundId = "rbxassetid://" .. audioID
                currentSound.Volume = 10
                currentSound:Play()

                task.delay(10000, function()
                    if currentSound then
                        currentSound:Stop()
                        currentSound:Destroy()
                        currentSound = nil
                    end
                end)
            end
        end

        PlayBtn.MouseButton1Click:Connect(function()
