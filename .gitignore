local TrollTab = Window:Tab({Title="Main", Icon="house"})


local TrollTab = Window:Tab({Title="Troll", Icon="clock"})
local selectedPlayer = nil
local flingEnabled = false
local teleporting = false
local flingSpeed = 999999999

-- Lista de jogadores
local function updatePlayerList()
    local list = {}
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= player then
            table.insert(list, plr.Name)
        end
    end
    return list
end

local dropdown = TrollTab:Dropdown({
    Title = "Selecionar Jogador",
    Options = updatePlayerList(),
    Default = "",
    Callback = function(value)
        selectedPlayer = Players:FindFirstChild(value)
    end
})

TrollTab:Button({
    Title = "Atualizar Lista de Jogadores",
    Callback = function()
        dropdown:Refresh(updatePlayerList(), true)
    end
})

-- Teleporte + Fling
local function startTeleport()
    teleporting = true

    while teleporting do
        task.wait(0.05)

        if not selectedPlayer or not selectedPlayer.Character then
            teleporting = false
            WindUI:Notify("Erro", "Jogador invÃ¡lido ou nÃ£o carregado!", 3)
            return
        end

        local targetHRP = selectedPlayer.Character:FindFirstChild("HumanoidRootPart")
        local myHRP = player.Character and player.Character:FindFirstChild("HumanoidRootPart")

        if not myHRP or not targetHRP then
            teleporting = false
            return
        end

        -- Seguir o alvo
        myHRP.CFrame = targetHRP.CFrame

        -- Ativar FLING
        if flingEnabled then
            myHRP.Velocity = Vector3.new(
                math.random(-flingSpeed, flingSpeed),
                flingSpeed * 2,
                math.random(-flingSpeed, flingSpeed)
            )
            myHRP.RotVelocity = Vector3.new(
                flingSpeed * 1.5,
                flingSpeed * 1.5,
                flingSpeed * 1.5
            )
        end
    end
end

local function stopTeleport()
    teleporting = false
end

-- ðŸ”¥ FLING + TELEPORT NO MESMO TOGGLE ðŸ”¥
TrollTab:Toggle({
    Title = "Fling Couch",
    Default = false,
    Callback = function(state)
        if state then
            if not selectedPlayer then
                WindUI:Notify("Erro", "Selecione um jogador primeiro!", 3)
                return
            end

            flingEnabled = true
            startTeleport()
        else
            flingEnabled = false
            stopTeleport()
        end
    end
})


local viewEnabled = false
local currentConnection

local function enableView(target)
    if not target or not target.Character then return end
    local hrp = target.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local cam = workspace.CurrentCamera
    cam.CameraSubject = hrp

    if currentConnection then currentConnection:Disconnect() end

    currentConnection = target.CharacterAdded:Connect(function(char)
        local newHRP = char:WaitForChild("HumanoidRootPart")
        cam.CameraSubject = newHRP
    end)
end

local function disableView()
    viewEnabled = false
    local cam = workspace.CurrentCamera
    cam.CameraSubject = player.Character:FindFirstChild("Humanoid")

    if currentConnection then
        currentConnection:Disconnect()
        currentConnection = nil
    end
end

TrollTab:Toggle({
    Title = "View",
    Default = false,
    Callback = function(value)
        viewEnabled = value
        if viewEnabled then
            enableView(selectedPlayer)
        else
            disableView()
        end
    end
})


local ESPTab = Window:Tab({Title="ESP", Icon="eye"})
local ESPObjects = {}
local espActive = false

local function createESP(plr)
    if plr ~= player and plr.Character then
        local highlight = Instance.new("Highlight")
        highlight.Adornee = plr.Character
        highlight.FillColor = Color3.new(1,0,0)
        highlight.OutlineColor = Color3.new(1,1,1)
        highlight.FillTransparency = 0.5
        highlight.OutlineTransparency = 0
        highlight.Parent = game.CoreGui
        ESPObjects[plr.Name] = highlight
    end
end

local function updateESP()
    for _, plr in ipairs(Players:GetPlayers()) do
        if not ESPObjects[plr.Name] then createESP(plr) end
    end
end

Players.PlayerAdded:Connect(createESP)
Players.PlayerRemoving:Connect(function(plr)
    if ESPObjects[plr.Name] then ESPObjects[plr.Name]:Destroy() ESPObjects[plr.Name] = nil end
end)

ESPTab:Toggle({
    Title="Ativar ESP",
    Default=false,
    Callback=function(state)
        espActive = state
        updateESP()
        for _, highlight in pairs(ESPObjects) do
            highlight.Enabled = espActive
        end
    end
})

ESPTab:Button({Title="ESP Vermelho", Callback=function() for _, h in pairs(ESPObjects) do h.FillColor = Color3.new(1,0,0) end end})
ESPTab:Button({Title="ESP Azul", Callback=function() for _, h in pairs(ESPObjects) do h.FillColor = Color3.new(0,0,1) end end})
ESPTab:Button({Title="ESP Verde", Callback=function() for _, h in pairs(ESPObjects) do h.FillColor = Color3.new(0,1,0) end end})
ESPTab:Button({Title="ESP Amarelo", Callback=function() for _, h in pairs(ESPObjects) do h.FillColor = Color3.new(1,1,0) end end})



